{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/app.py"
    },
    {
      "source": "/(.*)",
      "destination": "/app"
    }
  ],
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    },
    {
      "src": "/app/**/*",
      "use": "@vercel/next"
    }
  ]
}