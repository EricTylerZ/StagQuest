{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/app.py"
    },
    {
      "source": "/(.*)",
      "destination": "/app"
    }
  ],
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "functions": {
    "app.py": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}